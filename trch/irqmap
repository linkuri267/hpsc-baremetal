// Syntax per line: irq[:isr_name]|irq_from-irq_to

// We multiplex events from all mailboxes (in one IP block) onto one IRQ pair

#include "hwinfo.h"
#include "mailbox-map.h"

#if TEST_RTPS_TRCH_MAILBOX
MBOX_LSIO__IRQ_START+MBOX_HPPS_TRCH__TRCH_RCV_INT : mbox_hpps_rcv_isr
MBOX_LSIO__IRQ_START+MBOX_HPPS_TRCH__TRCH_ACK_INT : mbox_hpps_ack_isr
#endif

#if TEST_HPPS_TRCH_MAILBOX | TEST_HPPS_TRCH_MAILBOX_SSW
MBOX_HPPS_TRCH__IRQ_START+MBOX_HPPS_TRCH__TRCH_RCV_INT : mbox_lsio_rcv_isr
MBOX_HPPS_TRCH__IRQ_START+MBOX_HPPS_TRCH__TRCH_ACK_INT : mbox_lsio_ack_isr
#endif

#if TEST_TRCH_DMA
TRCH_DMA_EV0_IRQ : dma_trch_dma_event_0_isr
TRCH_DMA_ABORT_IRQ : dma_trch_dma_abort_isr
#endif

#if TEST_TRCH_WDT | TEST_TRCH_WDT_STANDALONE
WDT_TRCH_ST1_IRQ : wdt_trch_st1_isr
WDT_TRCH_ST2_IRQ : wdt_trch_st2_isr
#endif

#if TEST_RTPS_WDT | TEST_RTPS_WDT_STANDALONE
WDT_RTPS0_ST2_IRQ : wdt_rtps0_st2_isr
#endif
